<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="AcuantHybridSampleSDK.ResultPage">
	<ContentPage.Content>
		<StackLayout>
			<StackLayout>
				<Button x:Name="backButton" Text="Back"  Clicked="OnBackClicked"></Button>
				<StackLayout Orientation="Horizontal">
					<Image IsVisible="false" x:Name="frontImage" WidthRequest="150" HeightRequest="100"></Image>
					<Image IsVisible="false" x:Name="backImage"  WidthRequest="150" HeightRequest="100"></Image>
				</StackLayout>
				<StackLayout Orientation="Horizontal">
					<Image IsVisible="false" x:Name="faceImage"  WidthRequest="100" HeightRequest="100"></Image>
					<Image IsVisible="false" x:Name="signImage"  WidthRequest="100" HeightRequest="30"></Image>
				</StackLayout>
				<Button IsVisible="false" x:Name="scanEchip" Text="Scan eChip"  Clicked="OnScanEChipClicked"></Button>
			</StackLayout>
			<StackLayout>
				<ScrollView>
					<Label x:Name="dataLabel"></Label>
				</ScrollView>
			</StackLayout>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>
